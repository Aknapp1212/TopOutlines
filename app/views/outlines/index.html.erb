<p id="notice"><%= notice %></p>

<h1>My Outlines</h1>

<table class = "table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Content</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outlines.each do |outline| %>
      <tr>
        <td><%= outline.name %></td>
        <td><%= outline.content %></td>
        <td><%= link_to 'Show', outline %></td>
        <td><%= link_to 'Edit', edit_outline_path(outline) %></td>
        <td><%= link_to 'Destroy', outline, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

